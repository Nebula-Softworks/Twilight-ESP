{
  "name": "twilight-esp",
  "version": "1.2.0",
  "description": "The Extrasensory Star",
  "main": "src/init.luau",
  "scripts": {
    "dev-bundle": "lune run build/init.development.lua",
    "dev-server": "concurrently \"python -m http.server 8642\" \"chokidar \\\"src/**/*.luau\\\" -c \\\"pnpm dev-bundle\\\"\"",

    "buildmodel": "mkdirp dist && rojo build build.project.json -o dist/Twilight.ESP.rbxm",
    "bundle": "pnpm buildmodel && lune run build && del dist\\main.luau 2>nul"
  },
  "keywords": [],
  "author": "Nebula Softworks",
  "repository": "https://github.com/Nebula-Softworks/Twilight-ESP",
  "discord": "https://dsc.gg/nebulasoftworks",
  "license": "CC-BY-NC 4.0",
  "packageManager": "pnpm@10.15.1",
  "devDependencies": {
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.1",
    "mkdirp": "^3.0.1"
  }
}
